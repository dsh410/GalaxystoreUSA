<h1>{{section.settings.heading}}</h1>
<h3>{{section.settings.questions}}</h3>
<p>{{section.settings.answer}}</p>



{% schema %}
{
    "name":"customer questions",
    "settings":[
        {
            "type": "text",
            "label":"heading for faq",
            "id":"heading"
        },
        {
            "type": "text",
            "label":"customer questions",
            "id":"questions"
        },
        {
            "type": "textarea",
            "label":"customer answer",
            "id":"answer"
        }
    ]
}
{% endschema %}
